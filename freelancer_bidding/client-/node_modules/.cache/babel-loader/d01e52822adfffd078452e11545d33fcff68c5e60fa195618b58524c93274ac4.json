 axios from 'axios';\n\nexport default {\n  data() {\n    return {\n      loading: true,\n      projects: [],\n    };\n  },\n  async mounted() {\n    try {\n      const response = await axios.get('http://127.0.0.1:5000/projects');\n      this.projects = response.data;\n    } catch (error) {\n      console.error(error);\n    } finally {\n      this.loading = false;\n    }\n  },\n};\n</script>"],"mappings":"AA4CA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,OAAO,EAAE,IAAI;MACbC,QAAQ,EAAE;IACZ,CAAC;EACH,CAAC;EACD,MAAMC,OAAOA,CAAA,EAAG;IACd,IAAI;MACF,MAAMC,QAAO,GAAI,MAAML,KAAK,CAACM,GAAG,CAAC,gCAAgC,CAAC;MAClE,IAAI,CAACH,QAAO,GAAIE,QAAQ,CAACJ,IAAI;IAC/B,EAAE,OAAOM,KAAK,EAAE;MACdC,OAAO,CAACD,KAAK,CAACA,KAAK,CAAC;IACtB,UAAU;MACR,IAAI,CAACL,OAAM,GAAI,KAAK;IACtB;EACF;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}