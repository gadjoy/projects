\n        this.projects = response.data;\n      } catch (error) {\n        console.error('Error fetching projects:', error);\n      }\n    }\n  }\n};\n</script>\n\n<style scoped>\n/* Add your CSS styles here */\n</style>\n"],"mappings":"AA2BA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,EAAE;MACTC,KAAK,EAAE,CAAC;MAAE;MACVC,QAAQ,EAAE;IACZ,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,aAAaA,CAAA,EAAG;MACpB,IAAI;QACF,MAAMC,QAAO,GAAI,MAAMP,KAAK,CAACQ,GAAG,CAAC,uCAAuC,EAAE;UACxEC,MAAM,EAAE;YACNP,KAAK,EAAE,IAAI,CAACA,KAAK;YACjBC,KAAK,EAAE,IAAI,CAACA;UACd;QACF,CAAC,CAAC;QACF,IAAI,CAACC,QAAO,GAAIG,QAAQ,CAACN,IAAI;MAC/B,EAAE,OAAOS,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,0BAA0B,EAAEA,KAAK,CAAC;MAClD;IACF;EACF;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}