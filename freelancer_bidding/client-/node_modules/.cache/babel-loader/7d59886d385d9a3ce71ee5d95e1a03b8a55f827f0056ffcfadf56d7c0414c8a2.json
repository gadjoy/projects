{"ast":null,"code":"import { ref } from \"vue\";\nimport { createDf } from \"@/components/dataframe\";\nexport default {\n  setup() {\n    const query = ref(\"\");\n    const limit = ref(10);\n    const loading = ref(false);\n    const projects = ref([]);\n    const searchProjects = async () => {\n      loading.value = true;\n      const df = await createDf(query.value, limit.value);\n      projects.value = df.data;\n      loading.value = false;\n    };\n    return {\n      query,\n      limit,\n      loading,\n      projects,\n      searchProjects\n    };\n  }\n};","map":{"version":3,"names":["ref","createDf","setup","query","limit","loading","projects","searchProjects","value","df","data"],"sources":["/home/waseem/project/freelancer_bidding_automation/client-/src/App.vue"],"sourcesContent":["<template>\n  <div>\n    <div class=\"container\">\n      <div class=\"row\">\n        <div class=\"col-md-12\">\n          <h1>Search Projects</h1>\n          <form @submit.prevent=\"searchProjects\">\n            <div class=\"form-group\">\n              <label for=\"query\">Query</label>\n              <input type=\"text\" class=\"form-control\" id=\"query\" v-model=\"query\" />\n            </div>\n            <div class=\"form-group\">\n              <label for=\"limit\">Limit</label>\n              <input type=\"number\" class=\"form-control\" id=\"limit\" v-model=\"limit\" />\n            </div>\n            <button type=\"submit\" class=\"btn btn-primary\">Search</button>\n          </form>\n          <br />\n          <div v-if=\"loading\">Loading...</div>\n          <table class=\"table\" v-if=\"projects && !loading\">\n            <thead>\n              <tr>\n                <th>Name</th>\n                <th>Description</th>\n                <th>URL</th>\n              </tr>\n            </thead>\n            <tbody>\n              <tr v-for=\"project in projects\" :key=\"project.id\">\n                <td>{{ project.name }}</td>\n                <td>{{ project.description }}</td>\n                <td>{{ project.url }}</td>\n              </tr>\n            </tbody>\n          </table>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { ref } from \"vue\";\nimport { createDf } from \"@/components/dataframe\";\n\nexport default {\n  setup() {\n    const query = ref(\"\");\n    const limit = ref(10);\n    const loading = ref(false);\n    const projects = ref([]);\n\n    const searchProjects = async () => {\n      loading.value = true;\n      const df = await createDf(query.value, limit.value);\n      projects.value = df.data;\n      loading.value = false;\n    };\n\n    return {\n      query,\n      limit,\n      loading,\n      projects,\n      searchProjects,\n    };\n  },\n};\n</script>\n\n<style scoped>\ntable {\n  width: 100%;\n}\n\nth,\ntd {\n  padding: 8px;\n  text-align: left;\n}\n</style>"],"mappings":"AA0CA,SAASA,GAAE,QAAS,KAAK;AACzB,SAASC,QAAO,QAAS,wBAAwB;AAEjD,eAAe;EACbC,KAAKA,CAAA,EAAG;IACN,MAAMC,KAAI,GAAIH,GAAG,CAAC,EAAE,CAAC;IACrB,MAAMI,KAAI,GAAIJ,GAAG,CAAC,EAAE,CAAC;IACrB,MAAMK,OAAM,GAAIL,GAAG,CAAC,KAAK,CAAC;IAC1B,MAAMM,QAAO,GAAIN,GAAG,CAAC,EAAE,CAAC;IAExB,MAAMO,cAAa,GAAI,MAAAA,CAAA,KAAY;MACjCF,OAAO,CAACG,KAAI,GAAI,IAAI;MACpB,MAAMC,EAAC,GAAI,MAAMR,QAAQ,CAACE,KAAK,CAACK,KAAK,EAAEJ,KAAK,CAACI,KAAK,CAAC;MACnDF,QAAQ,CAACE,KAAI,GAAIC,EAAE,CAACC,IAAI;MACxBL,OAAO,CAACG,KAAI,GAAI,KAAK;IACvB,CAAC;IAED,OAAO;MACLL,KAAK;MACLC,KAAK;MACLC,OAAO;MACPC,QAAQ;MACRC;IACF,CAAC;EACH;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}